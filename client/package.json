  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "lint:prettier": "prettier --check './src/**/*.{js,jsx,ts,tsx}'",
    "lint:eslint": "eslint src/",
    "lint:tsc": "tsc --noEmit",
    "lint:all": "prettier --check './src/**/*.{js,jsx,ts,tsx}' && eslint src/ && tsc --noEmit",
    "lint:fix": "prettier --write './src/**/*.{js,jsx,ts,tsx}' && eslint src/ --fix && tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/__tests__/test-utils/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    }
  }
